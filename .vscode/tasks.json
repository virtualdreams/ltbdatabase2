{
    "version": "0.1.0",
    "command": "dotnet",
    "isShellCommand": true,
    "args": [],
    "tasks": [
        {
            "taskName": "build",
            "args": [
                "${workspaceRoot}\\project.json"
            ],
            "isBuildCommand": true,
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "publish",
            "args": [
                "--configuration",
                "Release",
                "${workspaceRoot}\\project.json"
            ],
            "isBuildCommand": false,
            "showOutput": "always",
            "echoCommand": true
        }
    ]
}